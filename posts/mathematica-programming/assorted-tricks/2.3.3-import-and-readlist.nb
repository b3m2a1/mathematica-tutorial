            RowBox[{"{", "}"}]}], "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{"Replace", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"ReadLine", "@", "textStream"}], ",", 
            RowBox[{"{", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"EndOfFile", ":>", 
               RowBox[{"Return", "[", "]"}]}], ",", "\[IndentingNewLine]", 
              RowBox[{"s_String", "\[RuleDelayed]", "\[IndentingNewLine]", 
               RowBox[{"StringReplace", "[", 
                RowBox[{"s", ",", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"key__", "~~", "\"\<  \>\"", "~~", "desc__", "~~", 
                   RowBox[{"(", 
                    RowBox[{"\"\<  \>\"", "|", "EndOfString"}], ")"}]}], 
                  "\[RuleDelayed]", 
                  RowBox[{"Sow", "@", 
                   RowBox[{"{", 
                    RowBox[{"key", ",", "desc"}], "}"}]}]}]}], "]"}]}]}], 
             "\[IndentingNewLine]", "}"}]}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"StringReplace", "[", 
           RowBox[{
            RowBox[{"First", "@", "f"}], ",", "\[IndentingNewLine]", 
